<h1>Index View</h1>

<div class="row">
  <% @bookmarks.each do |bookmark| %>
  <div class="col-md-3">
    <div class="thumbnail">
      <img src="http://placehold.it/300x200">
      <div class="caption">
        <h3><%= link_to bookmark.name, bookmark %></h3>
        <h3><%= bookmark.url %></h3>
      </div>
    </div>
  </div>
  <% end %>
</div>
